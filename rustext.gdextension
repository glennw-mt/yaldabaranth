[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://rustext/target/debug/librustext.so"
linux.release.x86_64 =   "res://rustext/target/release/librustext.so"
windows.debug.x86_64 =   "res://rustext/target/debug/rustext.dll"
windows.release.x86_64 = "res://rustext/target/release/rustext.dll"
macos.debug =            "res://rustext/target/debug/librustext.dylib"
macos.release =          "res://rustext/target/release/librustext.dylib"
macos.debug.arm64 =      "res://rustext/target/debug/librustext.dylib"
macos.release.arm64 =    "res://rustext/target/release/librustext.dylib"
